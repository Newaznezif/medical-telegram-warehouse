# Environment
ENVIRONMENT=development
DEBUG=True

# API
HOST=0.0.0.0
PORT=8000

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost/medical_warehouse

# Logging
LOG_LEVEL=DEBUG
LOG_FORMAT=json

# Redis
REDIS_URL=redis://localhost:6379/0

# File Upload
UPLOAD_DIR=uploads
MAX_UPLOAD_SIZE=10485760

# Analytics
ANALYTICS_CACHE_TTL=300

# YOLO
YOLO_MODEL_PATH=yolov8n.pt
YOLO_CONFIDENCE_THRESHOLD=0.5
